<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<MemeDef>
		<defName>AM_Structure_Atheist</defName>
		<label>atheism</label>
		<description>There are no gods, no supernatural beings guiding our path or watching over us. Unfalsifiable concepts cannot be used as a basis for a rational society.</description>
		<category>Structure</category>
		<iconPath>UI/Structures/AM_Atheist</iconPath>
		<impact>0</impact>
		<worshipRoomLabel>forum</worshipRoomLabel>
		<randomizationSelectionWeightFactor>3</randomizationSelectionWeightFactor>
		<deityCount>0</deityCount>
		<generalRules>
			<rulesStrings>
				<li>memeAdjective->atheist</li>
				<li>memeAdjective->godless</li>
				<li>memeAdjective->irreligious</li>
				<li>memeAdjective->nonreligious</li>
				<li>memeAdjective->unbelieving</li>
				<li>memeAdjective->secular</li>
				<li>memeLeaderNoun->overseer</li>
				<li>memeLeaderNoun->theorizer</li>
				<li>memeLeaderAdjective->secular</li>
				<li>memeLeaderAdjective->laic</li>
				<li>memeMoralist->theoretician</li>
				<li>memeMoralist->thinker</li>
				<li>memeMoralist->moralist</li>
				<li>memeMoralist->speculator</li>
				<li>memeMoralist->theorist</li>
				<li>memeMoralist->academic</li>
				<li>memeMoralist->dialectician</li>
			</rulesStrings>
		</generalRules>
		<descriptionMaker>
			<patterns>
				<li>
					<def>AM_AtheismDescription</def>
				</li>
			</patterns>
			<constants>
				<li>
					<key>structure_esotericism</key>
					<value>true</value>
				</li>
			</constants>
		</descriptionMaker>
		<symbolPacks>
			<li>
				<theme>atheism</theme>
				<adjective>atheist</adjective>
				<member>atheist</member>
			</li>
		</symbolPacks>
		<thingStyleCategories>
			<li>
				<category>AM_Atheist</category>
				<priority>4</priority>
			</li>
		</thingStyleCategories>
	</MemeDef>
	<MemeDef>
		<defName>AM_Structure_Jewish</defName>
		<label>jewish origins</label>
		<description>This ideoligion is a descendant of ancient Judaism. It may have very different precepts than those of thousands of years ago, but its symbols are similar.</description>
		<category>Structure</category>
		<iconPath>UI/Structures/AM_Jewish</iconPath>
		<impact>0</impact>
		<deityNameMakerOverride>AM_NamerDeityJewish</deityNameMakerOverride>
		<deityTypeMakerOverride>DietyTypeMakerAbrahamic</deityTypeMakerOverride>
		<worshipRoomLabel>synagogue</worshipRoomLabel>
		<randomizationSelectionWeightFactor>3</randomizationSelectionWeightFactor>
		<deityCount>1</deityCount>
		<generalRules>
			<rulesStrings>
				<li>memeAdjective->traditional</li>
				<li>memeAdjective->orthodox</li>
				<li>memeAdjective->rabbinical</li>
				<li>memeAdjective->spiritual</li>
