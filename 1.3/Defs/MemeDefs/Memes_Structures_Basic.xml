			<li>
				<theme>judaism</theme>
				<adjective>jewish</adjective>
				<member>jew</member>
			</li>
		</symbolPacks>
		<thingStyleCategories>
			<li>
				<category>AM_Jewish</category>
				<priority>4</priority>
			</li>
		</thingStyleCategories>
	</MemeDef>
