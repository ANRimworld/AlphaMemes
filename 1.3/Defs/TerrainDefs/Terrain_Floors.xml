<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<!-- Jewish -->
	
	<DesignatorDropdownGroupDef>
		<defName>AM_Floor_JewishTiles</defName>
		<label>jewish stone tiles</label>
	</DesignatorDropdownGroupDef>
	
	<TerrainDef Abstract="True" ParentName="FloorBase" Name="AM_JewishTileStoneBase">
    <renderPrecedence>220</renderPrecedence>
    <description>Carefully-cut and fit stone tiles in a Jewish style. They are pretty to look at, but they take a long time to lay.</description>
    <texturePath>Terrain/Surfaces/AM_JewishTile</texturePath>
    <statBases>
      <WorkToBuild>1100</WorkToBuild>
      <Beauty>1</Beauty>
    </statBases>
	 <canGenerateDefaultDesignator>false</canGenerateDefaultDesignator>
    <designatorDropdown>AM_Floor_JewishTiles</designatorDropdown>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
	<dominantStyleCategory>AM_Jewish</dominantStyleCategory>
    <constructionSkillPrerequisite>3</constructionSkillPrerequisite>
  </TerrainDef>
  
  <TerrainDef ParentName="AM_JewishTileStoneBase">
    <defName>AM_JewishTileSandstone</defName>
    <label>jewish sandstone tile</label>
    <color>(126,104,94)</color>
    <costList>
      <BlocksSandstone>4</BlocksSandstone>
    </costList>
  </TerrainDef>

  <TerrainDef ParentName="AM_JewishTileStoneBase">
