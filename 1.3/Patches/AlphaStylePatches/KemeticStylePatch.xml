<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Weapons Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>


				<li Class="PatchOperationConditional">
					<success>Always</success>
					<xpath>/Defs/StyleCategoryDef[defName = "AM_Kemetic"]/thingDefStyles</xpath>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/StyleCategoryDef[defName = "AM_Kemetic"]/thingDefStyles</xpath>
						<value>
							
							<li>
								<thingDef>VWE_Gun_ChargePistol</thingDef>
								<styleDef>AM_Kemetic_ChargePistol</styleDef>
							</li>
							<li>
								<thingDef>VWE_Gun_ChargeShotgun</thingDef>
								<styleDef>AM_Kemetic_ChargeShotgun</styleDef>
							</li>
							<li>
								<thingDef>VWE_Gun_ChargeSMG</thingDef>
								<styleDef>AM_Kemetic_ChargeSMG</styleDef>
							</li>
							<li>
								<thingDef>VWE_Gun_ChargeLMG</thingDef>
								<styleDef>AM_Kemetic_ChargeLMG</styleDef>
							</li>
							<li>
								<thingDef>VWE_Gun_ChargeSniperRifle</thingDef>
								<styleDef>AM_Kemetic_ChargeSniperRifle</styleDef>
							</li>
							<li>
								<thingDef>VWE_Gun_ChargeMinigun</thingDef>
								<styleDef>AM_Kemetic_ChargeMinigun</styleDef>
							</li>
							<li>
								<thingDef>VWE_Gun_AntiMaterialRifle</thingDef>
								<styleDef>AM_Kemetic_AntiMaterialRifle</styleDef>
							</li>
							<li>
								<thingDef>VWE_Bow_Compound</thingDef>
								<styleDef>AM_Kemetic_CompoundBow</styleDef>
							</li>
							<li>
								<thingDef>VWE_Bow_Long</thingDef>
								<styleDef>AM_Kemetic_LongBow</styleDef>
							</li>
							

						</value>
					</match>
				</li>


			</operations>
		</match>
	</Operation>






</Patch>